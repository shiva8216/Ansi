- name: variables
  hosts: local
  connection: local
  vars: # Play level variables
    GREET: " play"
  vars_files:
    - course.yaml
  vars_prompt:
  - name: GREET
    prompt: Enter the value
    private: false
  tasks:
  - name: print the variable
    # vars:
    #   GREET: "TASK"
    ansible.builtin.debug:
      # msg: "Hello from {{ GREET }}"

